Action add(y){
	get x from count;
	put x+y to count;
}

Agent search_uav {
	add;
}

Behavior add_one(){
	@init{}
	@goal{}
	@routine{
		add(1);
	}
}

Task search({agtC[stC~edC]}){
	@init{
		put 0 to count;
	}
	@goal{}
	@routine{
		each agtC[stC~edC] {
			add_one();
		}
	}
}

Main {
	Agent search_uav 3;
	search({search_uav[0~3]});
}