Port : 41451
/* test comments*/
Action add_Action(s){
	s = s+1;
    return s;
}

Agent search_uav { 
	add_Action;
}

Behavior flyCircle_Behavior(){
	@init{
        step = 0;
	}
	@goal{
		$ step == 5
	}
	@routine{
		step = add_Action(step);
	}
}

Task search({agtC[stC~edC]}){
	@init{}
	@goal{}
	@routine{
		order agtC[stC~edC] {
			flyCircle_Behavior();
		}
	}
}

Main {
	Agent search_uav 3;
	search({search_uav[0~3]});
}