Port : 41451

Action takeOff_Action(){
	:= takeOff_API();
}

Action flyToHeight(h){
	:= flyToHeight_API(h);
}

Agent testUav{
    takeOff_Action, flyToHeight;
}

Behavior reach(){
    @init{ }
    @goal{ }
    @routine{
        takeOff_Action();
        flyToHeight(15);
    }
}

Task reach_in_order({agtA[stA~edA]}) {
    @init{ }
    @goal{
    }
    @routine{
        each agtA[stA~edA]{
            reach();
        }
    }
}

Main {
    Agent testUav 5;
    reach_in_order({testUav[0~3]});
}