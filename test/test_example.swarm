Port : 41451

Action trees_go_action(){
    := takeoff();
}

Agent order_uav {
    trees_go_action;
}

Agent each_uav {
    trees_go_action;
}

Behavior trees_go_behavior(){
    @init{ }
    @goal{ }
    @routine{
        trees_go_action();
    }
}

Task test_task({agtA[stA~edA], agtB[stB~edB]}){
    @init{}
    @goal{}
    @routine{
        order agtA[stA~edA] {
            trees_go_behavior();
        }
        each agtB[stB~edB-1] {
            trees_go_behavior();
        }
    }
}

Main {
    Agent order_uav 3;
    Agent each_uav 6;
    test_task({order_uav[0~3], each_uav[0~5]});
}